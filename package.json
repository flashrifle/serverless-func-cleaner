{
  "name": "serverless-func-cleaner",
  "version": "1.0.0",
  "description": "A tool to clean up old versions of serverless functions, keeping only the latest version",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "clean": "node src/index.js --dry-run"
  },
  "keywords": [
    "serverless",
    "lambda",
    "aws",
    "cleanup",
    "version-management",
    "serverless-framework"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.450.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
